<% if (success && success.length) { %>
<h5 class="alert alert-success alert-dismissible fade show  col-6 offset-3" role="alert">
    <%=success%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</h5>
<% } %>

<% if (failure && failure.length) { %>
<h5 class="alert alert-danger alert-dismissible fade show  col-6 offset-3" role="alert">
    <%=failure%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</h5>
<% } %>